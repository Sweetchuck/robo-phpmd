{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-phpmd",
    "description": "Robo task wrapper for PHPMD commands",
    "keywords": [
        "robo-tasks",
        "phpmd",
        "php-mess-detector"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "support": {
        "source": "https://github.com/Sweetchuck/robo-phpmd",
        "issues": "https://github.com/Sweetchuck/robo-phpmd/issues"
    },
    "license": "GPL-2.0-or-later",
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "consolidation/robo": "^3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.1",
        "mikey179/vfsstream": "^1.6",
        "pdepend/pdepend": "dev-master as 2.11.0",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "2.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "2.x-dev",
        "sweetchuck/robo-phpcs": "2.x-dev",
        "symfony/error-handler": "4.0 || ^5.0",
        "symfony/finder": "^4.0 || ^5.0",
        "symfony/yaml": "^4.0 || ^5.0"
    },
    "suggest": {
        "phpmd/phpmd": "After all the 'phpmd' executable has to be available."
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\PhpMessDetector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\PhpMessDetector\\Test\\": "tests/_support/",
            "Sweetchuck\\Robo\\PhpMessDetector\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\PhpMessDetector\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
